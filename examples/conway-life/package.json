{
  "name": "conway-life-example",
  "version": "0.1.0",
  "description": "Conway's Game of Life with real-time multiplayer using the game engine",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "dev:client": "ts-node src/client-demo.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest"
  },
  "dependencies": {
    "@multiplayer-engine/core": "workspace:*",
    "@multiplayer-engine/grid": "workspace:*",
    "@multiplayer-engine/realtime": "workspace:*",
    "express": "^4.18.0",
    "http": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "conway",
    "game-of-life",
    "multiplayer",
    "real-time",
    "game-engine"
  ],
  "license": "MIT"
}